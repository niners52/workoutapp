{
  "expo": {
    "name": "WorkoutTracker",
    "slug": "WorkoutTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "plugins": [
      [
        "react-native-health",
        {
          "isClinicalDataEnabled": false
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tylerr82.workouttracker8052",
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.background-delivery": true
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSHealthShareUsageDescription": "WorkoutTracker needs access to your health data to show your workout history and track your fitness progress.",
        "NSHealthUpdateUsageDescription": "WorkoutTracker will save your completed workouts to Apple Health."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8fc23c5a-aae4-4269-ac5d-bc77ad6dd97b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/8fc23c5a-aae4-4269-ac5d-bc77ad6dd97b"
    }
  }
}
